{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "0.0.0.1",
    "parameters": {
        "serviceBusNamespaceName": {
            "type": "string",
            "defaultValue": "tswServiceBusNamespace"
        },
        "rgName": {
      "type": "string"
    },
    "tswPlanBranchName": {
      "type": "string",
      "defaultValue": "Master"
    },
    "tswTestPlanName": {
      "type": "string"
    },
    "testPlanSuite": {
      "type": "string"
    },
    "tswPlanBuildVersionNumber": {
      "type": "string"
    },
    "workGroupName": {
      "type": "string",
      "defaultValue": "Master"
    },
    "dashboardVersion": {
      "type": "string",
      "defaultValue": "latest"
    },
    "storageKey": {
      "type": "string"
    },
    "spmVersion": {
      "type": "string",
      "defaultValue": "latest"
    },
    "sposVersionNumber": {
      "type": "string",
      "defaultValue": "latest"
    },
    "tagsEnvironment": {
      "type": "string"
    },
    "tagsGeo": {
      "type": "string"
    },
    "tagsEntity": {
      "type": "string"
    },
    "tagsType": {
      "type": "string"
    },
    "tagsTeam": {
      "type": "string"
    },
    "storeIdentityManagementServiceUri": {
      "type": "string"
    },
    "mm2Uri": {
      "type": "string",
      "defaultValue": "https://pe-int-menumanager.test.subway.com/api/integrations/menu-status-update"
    },
    "RemoteOrderUrl": {
      "type": "string",
      "defaultValue": ""
    },
    "machineList": {
      "type": "array",
      "metadata": {
        "description": "List of vm information to run the different TSW plans"
      }
    },
    "adminUsername": {
      "type": "string",
      "defaultValue": "subwayposuser",
      "allowedValues": [
        "subwayposuser"
      ]
    },
    "adminPassword": {
      "type": "securestring",
      "defaultValue": "posuser@12345",
      "allowedValues": [
        "posuser@12345"
      ]
    },
    "virtualNetworkName": {
      "type": "string",
      "defaultValue": "A06-VNet",
      "allowedValues": [
        "A06-VNet"
      ]
    },
    "networkInterfaceName": {
      "type": "string",
      "defaultValue": "tsw-vmNic"
    },
    "subnetName": {
      "type": "string",
      "defaultValue": "mpos1-subnet",
      "allowedValues": [
        "mpos1-subnet"
      ]
    },
    "diagnosticsStorageAccountName": {
      "defaultValue": "azurecitresources",
      "type": "string"
    },
    "diagnosticsStorageAccountId": {
      "defaultValue": "/subscriptions/0536860d-808b-44d5-8234-7a0310dad290/resourceGroups/POS-DEV-AzureCit-RG/providers/Microsoft.Storage/storageAccounts/azurecitresources",
      "type": "string"
    },
    "enableIaaSDiagnostics": {
      "type": "bool",
      "defaultValue": false
    }
    },
    "variables": {},
    "functions": [],
    "resources": [
        {
            "type": "Microsoft.ServiceBus/namespaces",
            "apiVersion": "2017-04-01",
            "name": "[parameters('serviceBusNamespaceName')]",
            "location": "East US",
            "sku": {
                "name": "Standard",
                "tier": "Standard"
            },
            "properties": {
                "provisioningState": "Succeeded",
                "metricId": "0536860d-808b-44d5-8234-7a0310dad290:tsw-servicebus",
                "createdAt": "2019-04-22T13:04:55.57Z",
                "updatedAt": "2019-04-22T13:05:19.163Z",
                "serviceBusEndpoint": "[concat('https://', parameters('serviceBusNamespaceName'), '.servicebus.windows.net:443/')]",
                "status": "Active"
            }
        },
        {
            "type": "Microsoft.ServiceBus/namespaces/AuthorizationRules",
            "apiVersion": "2017-04-01",
            "name": "[concat(parameters('serviceBusNamespaceName'), '/RootManageSharedAccessKey')]",
            "location": "East US",
            "dependsOn": [
                "[resourceId('Microsoft.ServiceBus/namespaces', parameters('serviceBusNamespaceName'))]"
            ],
            "properties": {
                "rights": [
                    "Listen",
                    "Manage",
                    "Send"
                ]
            }
        },
        {
            "type": "Microsoft.ServiceBus/namespaces/topics",
            "apiVersion": "2017-04-01",
            "name": "[concat(parameters('serviceBusNamespaceName'), '/tsw-topics')]",
            "location": "East US",
            "dependsOn": [
                "[resourceId('Microsoft.ServiceBus/namespaces', parameters('serviceBusNamespaceName'))]"
            ],
            "properties": {
                "defaultMessageTimeToLive": "P14D",
                "maxSizeInMegabytes": 1024,
                "requiresDuplicateDetection": false,
                "duplicateDetectionHistoryTimeWindow": "PT10M",
                "enableBatchedOperations": true,
                "status": "Active",
                "supportOrdering": true,
                "autoDeleteOnIdle": "P10675199DT2H48M5.4775807S",
                "enablePartitioning": false,
                "enableExpress": false
            }
        },
        {
            "type": "Microsoft.ServiceBus/namespaces/topics/subscriptions",
            "apiVersion": "2017-04-01",
            "name": "[concat(parameters('serviceBusNamespaceName'), '/tsw-topics/tsw-subscription-', copyIndex('subscription'))]",
            "location": "East US",
            "dependsOn": [
                "[resourceId('Microsoft.ServiceBus/namespaces/topics', parameters('serviceBusNamespaceName'), 'tsw-topics')]",
                "[resourceId('Microsoft.ServiceBus/namespaces', parameters('serviceBusNamespaceName'))]"
            ],
            "properties": {
                "lockDuration": "PT30S",
                "requiresSession": false,
                "defaultMessageTimeToLive": "P14D",
                "deadLetteringOnMessageExpiration": false,
                "deadLetteringOnFilterEvaluationExceptions": false,
                "maxDeliveryCount": 10,
                "status": "Active",
                "enableBatchedOperations": false,
                "autoDeleteOnIdle": "P10675199DT2H48M5.4775807S"
            },
            "copy": {
                "name": "subscription",
                "count": 3
            }
        },
        {
            "type": "Microsoft.ServiceBus/namespaces/topics/subscriptions/rules",
            "apiVersion": "2017-04-01",
            "name": "[concat(parameters('serviceBusNamespaceName'), '/tsw-topics/tsw-subscription-', copyIndex('rule'), '/', 'rule', copyIndex('rule'))]",
            "location": "East US",
            "dependsOn": [
                "[resourceId('Microsoft.ServiceBus/namespaces/topics/subscriptions', parameters('serviceBusNamespaceName'), 'tsw-topics', concat('tsw-subscription-', copyIndex('rule')))]",
                "[resourceId('Microsoft.ServiceBus/namespaces/topics', parameters('serviceBusNamespaceName'), 'tsw-topics')]",
                "[resourceId('Microsoft.ServiceBus/namespaces', parameters('serviceBusNamespaceName'))]"
            ],
            "properties": {
                "action": {},
                "filterType": "SqlFilter",
                "sqlFilter": {
                    "sqlExpression": "1=1"
                }
            },
            "copy": {
                "name": "rule",
                "count": 3
            }
        }
    ],
    "outputs": {
        "ServiceBusConnectionString": {
            "type": "string",
            "value": "[listKeys(resourceId('Microsoft.ServiceBus/namespaces/AuthorizationRules', 'tsw-test', 'RootManageSharedAccessKey'), '2017-04-01').PrimaryConnectionString]"
        }
    }
}
